function action_regist( modal_id, modal_url, callback_url ) {
console.log('action_regist(%s,%s,%s)', modal_id, modal_url, callback_url);
$.colorbox({
href: modal_url,
iframe: true,
opacity: '0.5',
open: true,
scrolling: true,
height: '60%',
maxWidth: '90%',
width: $(window).width() - 60 + 'px',
