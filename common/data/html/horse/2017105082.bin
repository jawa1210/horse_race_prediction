if('id' == val[0]){
horse_id = val[1];
}
}
}
if(undefined == horse_id){
const path = window.location.pathname.split('/');
for(let i=0; i < path.length; i++){
if('horse' == path[i] && ! path[i+1].match(/\.html/)){
horse_id = path[i+1];
}
}
}
$.get('https://db.netkeiba.com/social/api_db_horse_info_simple.html',
{
input: 'UTF-8',
output: 'json',
id: horse_id,
},
function(html){
$('#HorseInfo').html(html);
});
});
</script>
